@model List<Casgem_Protfolio.Models.Entities.TblFeature>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_PortfolioAdminLayout.cshtml";
}

<h2>Ben Kimim(İlk Alan)</h2>
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Başlık</th>
        <th>Açıklama</th>
        <th>Resim</th>
        <th>Güncelle</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.FeatureID</td>
            <td>@item.FeatureTitle</td>
            <td>@item.FeatureDescription</td>
            <td>@item.FeatureImageURL</td>
            <td><a href="/WhoAmI/UpdateFeature/@item.FeatureID" class="btn btn-warning">Güncelle</a></td>
        </tr>
    }

</table>


